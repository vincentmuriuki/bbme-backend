{
  "name": "bnme-backend",
  "version": "1.0.0",
  "description": "bbme backend",
  "main": "./src/index.js",
  "scripts": {
    "start": "npm run create && SET NODE_ENV=staging babel-node ./src/index.js",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "create": "SET NODE_ENV=staging sequelize db:migrate",
    "test": "SET NODE_ENV=test npm run migrate && SET NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register --timeout 3000 ./src/test/*.js --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincentmuriuki/bbme-backend.git"
  },
  "author": "Vincent Muriuki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vincentmuriuki/bbme-backend/issues"
  },
  "homepage": "https://github.com/vincentmuriuki/bbme-backend#readme",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@sendgrid/mail": "^7.2.5",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.23.0",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "email-templates": "^7.1.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.4.11"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.6",
    "install": "^0.13.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.1.3",
